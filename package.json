  "scripts": {
    "ng:serve": "ng serve",
    "electron:serve-tsc": "tsc --p electron",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && cross-env NODE_ENV=dev electron electron/dist/main.js",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:prod": "npm run build -- -c production",
    "build:dev": "npm run build -- -c dev",
    "start:dev": "npm-run-all -p electron:serve ng:serve",
    "electron:local": "npm run build:dev && electron electron/dist/main.js",
    "electron:build-win-never": "npm run build:prod && node build.js --platform WINDOWS --publish never",
    "electron:build-win-publish": "npm run build:prod && node build.js --platform WINDOWS --publish always"
  }
